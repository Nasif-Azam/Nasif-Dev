# Fabric Deployment Configuration
# Copy this file to .env and fill in your actual values

# Azure Tenant ID
TENANT_ID_ENV=your-tenant-id-here

# Service Principal Client ID (Application ID)
CLIENT_ID_ENV=your-client-id-here

# Service Principal Client Secret
CLIENT_SECRET_ENV=your-client-secret-here

# Fabric Capacity ID (from your Fabric capacity)
CAPACITY_ID_ENV=your-capacity-id-here

# Workspace names (optional, defaults to Dev and Prod)
DEV_WORKSPACE_NAME=Nasif-Dev
PROD_WORKSPACE_NAME=Nasif-Prod

# Workspace IDs (optional - provide if you know the workspace GUIDs)
# This helps when the Service Principal can't list workspaces
DEV_WORKSPACE_ID=
PROD_WORKSPACE_ID=

# Skip role assignment (set to true if you already have admin role)
SKIP_ROLE_ASSIGNMENT=false
